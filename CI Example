import random
import statistics
y=[]
w=[]
n=1
m=1
import csv
while n<21:
	print(n) #he prints (n) to check if there's any breaks 
	while m<31:
		xd=random.normalvariate(19,1) #(Mu,sd)
		m=m+1
		w.append(xd)
	m=1
	x=statistics.mean(w)
	del w[:]
	y.append(x)
	n=n+1
print(statistics.mean(y))
print(statistics.stdev(y))
print(statistics.pstdev(y)/statistics.stdev(y)-1)
